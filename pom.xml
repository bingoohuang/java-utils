    <groupId>com.github.bingoohuang</groupId>
    <artifactId>java-utils</artifactId>
    <version>0.0.26-SNAPSHOT</version>
    <packaging>jar</packaging>

    <name>java utilities</name>
    <description>java utilities</description>
    <url>http://github.com/bingoohuang/java-utils</url>
    <properties>
        <java.version>1.8</java.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <maven.compiler.source>${java.version}</maven.compiler.source>
        <maven.compiler.target>${java.version}</maven.compiler.target>
    </properties>
    <licenses>
        <license>
            <name>The Apache Software License, Version 2.0</name>
            <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
            <distribution>repo</distribution>
        </license>
    </licenses>

    <scm>
        <connection>scm:git:git@github.com:bingoohuang/java-utils.git</connection>
        <url>http://github.com/bingoohuang/java-utils</url>
        <developerConnection>scm:git:git@github.com:bingoohuang/java-utils.git</developerConnection>
    </scm>

    <developers>
        <developer>
            <id>bingoohuang</id>
            <name>bingoo huang</name>
            <email>bingoo.huang@gmail.com</email>
